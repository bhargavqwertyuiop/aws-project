# AWS Configuration
REACT_APP_AWS_REGION=us-east-1

# AWS Cost Explorer
REACT_APP_CE_REGION=us-east-1

# AWS CloudWatch
REACT_APP_CW_REGION=us-east-1

# AWS Cognito Configuration
REACT_APP_COGNITO_USER_POOL_ID=your_user_pool_id_here
REACT_APP_COGNITO_CLIENT_ID=your_cognito_client_id_here
REACT_APP_COGNITO_REGION=us-east-1

# DynamoDB Configuration
REACT_APP_DYNAMODB_TABLE_NAME=aws-cost-optimizer
REACT_APP_DYNAMODB_REGION=us-east-1

# AWS SES Configuration
REACT_APP_SES_REGION=us-east-1
REACT_APP_SES_FROM_EMAIL=noreply@your-domain.com

# OpenAI API Configuration (for AI recommendations)
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here

# Application Configuration
REACT_APP_API_BASE_URL=https://your-backend-api.com
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_REAL_AWS_DATA=false
REACT_APP_ENABLE_AI_RECOMMENDATIONS=true
REACT_APP_ENABLE_EMAIL_REPORTS=true